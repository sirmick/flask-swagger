<html>
<head>
    <title>{{ title }}</title>

    <link href='http://fonts.googleapis.com/css?family=Droid+Sans:400,700' rel='stylesheet' type='text/css'/>
    <link href='{{static_dir}}/swagger-ui-1.1.7/css/hightlight.default.css' media='screen' rel='stylesheet' type='text/css'/>
    <link href='{{static_dir}}/swagger-ui-1.1.7/css/screen.css' media='screen' rel='stylesheet' type='text/css'/>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/jquery-1.8.0.min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/jquery.slideto.min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/jquery.wiggle.min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/jquery.ba-bbq.min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/handlebars-1.0.rc.1.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/underscore-min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/backbone-min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/swagger.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/swagger-ui.min.js' type='text/javascript'></script>
    <script src='{{static_dir}}/swagger-ui-1.1.7/lib/highlight.7.3.pack.js' type='text/javascript'></script>

    <style type="text/css">
        .swagger-ui-wrap {
            max-width: 960px;
            margin-left: auto;
            margin-right: auto;
        }

        .icon-btn {
            cursor: pointer;
        }

        #message-bar {
            min-height: 30px;
            text-align: center;
            padding-top: 10px;
        }

        .message-success {
            color: #89BF04;
        }

        .message-fail {
            color: #cc0000;
        }
    </style>

    <script type="text/javascript">
        $(function () {
            window.swaggerUi = new SwaggerUi({
                discoveryUrl:"{{url_prefix}}/meta/resources",
                dom_id:"swagger-ui-container",
                supportHeaderParams: true,
                supportedSubmitMethods: ['get', 'post', 'put', 'delete']
            });

            window.swaggerUi.load();
        });

    </script>
</head>

<body>
<div id='header'>
    <div class="swagger-ui-wrap">
        <a id="logo" href="http://swagger.wordnik.com">{{ title }}</a>
    </div>
</div>

<div id="message-bar" class="swagger-ui-wrap">
    &nbsp;
</div>

<div id="swagger-ui-container" class="swagger-ui-wrap">

</div>

</body>
</html>
